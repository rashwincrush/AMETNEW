{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": "warn"
  }
}
